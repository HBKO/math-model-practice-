%进行BP神经网络的训练
clear;
clc;
finaldata=importdata('finaldata.mat');
%选取1:400为训练数据，401：569为测试数据
p=finaldata(1:400,3:7)';
t=finaldata(1:400,2)';
net=newff([0 20;0 16;0 12;0 12;0 13],[3 1],{'logsig' 'purelin'},'traingdx');
net.trainparam.show=100;%两次显示之间的训练步数
net.trainparam.lr=0.5; %学习率
net.trainparam.mc=0.8; %惯性常熟
net.trainparam.epochs=20000;
net.trainparam.goal=0.01; %训练目标
[net,tr]=train(net,p,t);
Y=sim(net,p);
s